{
  "name": "hono",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "db:studio": "prisma studio --schema=./prisma/",
    "db:seed": "bun /prisma/seed.ts",
    "db:migrate": "bunx prisma migrate dev --name",
    "db:generate": "bunx prisma generate",
    "db:push": "prisma db push --schema=./prisma/",
    "tunels": "sudo cloudflared tunnel --url http://0.0.0.0:8080",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@telegraf/session": "^2.0.0-beta.7",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "hono": "^4.9.7",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "megajs": "^1.3.9",
    "prisma": "^6.16.2",
    "stream": "^0.0.3",
    "telegraf": "^4.16.3",
    "uuid": "^13.0.0",
    "vercel": "^48.1.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  }
}